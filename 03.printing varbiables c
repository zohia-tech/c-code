include <iostream>   // Library for input and output
using namespace std;  // Allows use of standard names without std::

int main()            // Main function where the program starts
{
    int x = 7;        // Declare an integer variable x and assign value 7
    cout << x << endl; // Print the value of x

    x = x + 6;        // Add 6 to x and store the new value in x
    cout << x << endl; // Print the updated value of x

    x = x - 20;       // Subtract 20 from x
    cout << x << endl; // Print the final value of x

    return 0;         // End the program successfully
}
