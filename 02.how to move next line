#include <iostream>   // This library is used for input and output
using namespace std;  // This allows us to use standard names without writing std::

int main() {          // The main function where program execution starts

    cout << "Hello Zo" << endl;   // Prints "Hello Zo" on the screen
    cout << endl;                // Prints a blank line
    cout << endl;                // Prints another blank line
    cout << "Hello Zoi" << endl; // Prints "Hello Zoi" on the screen

    return 0;        // Ends the program successfully
}
