include <iostream>   // Library used for input and output
using namespace std;  // Allows use of standard names without std::

int main() {          // Main function where the program starts

    // Loop to print numbers from 1 to 10
    for (int i = 1; i <= 10; i++) {
        cout << i << " "; // Print the value of i followed by a space
    }

    cout << endl;     // Move to the next line
    return 0;         // End the program successfully
}
